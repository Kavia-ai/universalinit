# Ignore the default SQLite database
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all log files and temp files.
/log/*
!/log/.keep
/tmp
!/tmp/.keep

# Ignore PID files, cache, and public/system files
/tmp/pids
/tmp/cache
/tmp/sockets
/public/system
/public/uploads

# Ignore compiled files
**/*.rbc

# Ignore byebug command history file
.byebug_history

# Ignore dotenv environment variables file
.env
.env.*

# Ignore node modules and Yarn files
/node_modules
/yarn-error.log

# Ignore package-lock and yarn lock if using only one
/package-lock.json
/yarn.lock

# Ignore coverage reports
/coverage/

# Ignore system test files
/spec/examples.txt

# Ignore Rails credentials and master key
/config/master.key
/config/credentials.yml.enc

# Ignore Docker-related files if they exist
.docker/
docker-compose.override.yml

# macOS and Linux system files
.DS_Store
*.swp

# Ignore RuboCop and Solargraph files
.rubocop-https*
.solargraph.yml

# Ignore test coverage
/coverage/
/spec/tmp

# Ignore uploaded files in development
/storage/*
!/storage/.keep

# Ignore Capybara screenshots
/tmp/capybara

# Ignore spring
/tmp/spring

# Ignore files from various editors and IDEs
*.iml
.idea/
.vscode/
*.sublime*

vendor/bundle